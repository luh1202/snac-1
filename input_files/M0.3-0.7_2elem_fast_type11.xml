        <param name="dikeDepth"> 8000 </param>	        

        <param name="M_type"> 11 </param><!--M_type along z axis 1:linear 2:sinusoidal 3:sqrt 4:()^(1/3) -->
        <param name="Mb"> 0.2 </param><!--Mb M-factor at z=0 -->
        <param name="Me"> 0.2 </param><!--Mb M-factor at z=zmax -->
